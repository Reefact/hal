<Project>
    <PropertyGroup>
        <Description>A C#/.NET Core implementation of Hypertext Application Language (HAL).</Description>
		<Copyright>Copyright (C) 2017-2024 by Reefact</Copyright>
		<Authors>Sylvain Aurat</Authors>
		<Nullable>enable</Nullable>
		<LangVersion>10.0</LangVersion>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<AssemblyOriginatorKeyFile>../hal.snk</AssemblyOriginatorKeyFile>
		<SignAssembly>true</SignAssembly>
		<RepositoryUrl>https://github.com/reefact/hal</RepositoryUrl>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<AssemblyVersion>0.999.0.0</AssemblyVersion>
		<FileVersion>0.999.0.0</FileVersion>
		<Version>0.999.0-dev</Version>
    </PropertyGroup>
</Project>
